@model IEnumerable<emps.Models.Position>

@{
    ViewBag.Title = "ListPositions";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>ListPositions</h2>
<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Skills</th>
        </tr>

    </thead>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Title</td>
            <td></td>
        </tr>
    }
</table>